{
  "expo": {
    "name": "Zukus",
    "slug": "zukus",
    "version": "1.0.0",
    "scheme": "zukus",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#2e1a47",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2e1a47"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.zukus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2e1a47"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.zukus"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
